{
  "name": "react-level-up",
  "version": "1.0.0",
  "description": "level-up react app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "eslint": "./node_modules/eslint/bin/eslint.js './src/*.js'",
    "test": "jest --detectOpenHandles --coverage",
    "babel": "babel app.js -d build",
    "heroku:start": "node build/app.js",
    "heroku-postbuild": "npm run babel && npm run build",
    "coverage": "jest --colors --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js",
      "./setup.js",
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      ".+\\.(png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwashangai/React-Level-Up.git"
  },
  "author": "Chiagoziem Young Nwadike",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nwashangai/React-Level-Up/issues"
  },
  "homepage": "https://github.com/nwashangai/React-Level-Up#readme",
  "dependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@babel/preset-react": "7.0.0-beta.51",
    "@babel/runtime": "7.0.0-beta.55",
    "axios": "0.18.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-stage-0": "6.24.1",
    "coveralls": "^3.0.2",
    "css-loader": "1.0.1",
    "dotenv": "6.2.0",
    "dotenv-webpack": "1.6.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "enzyme-to-json": "3.3.5",
    "express": "4.17.3",
    "fetch-mock": "^7.3.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.6.0",
    "jest": "24.0.0",
    "jest-css-modules-transform": "^2.1.1",
    "jest-transform-stub": "^1.0.0",
    "jwt-decode": "2.2.0",
    "moxios": "^0.4.0",
    "prop-types": "^15.6.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "^5.0.7",
    "react-router": "3.2.0",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.4",
    "react-test-renderer": "16.7.0",
    "react-toast-notifications": "^1.3.1",
    "react-toastify": "^4.5.2",
    "redux": "3.5.2",
    "redux-devtools-extension": "2.13.7",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "2.3.0",
    "sinon": "^7.2.3",
    "style-loader": "0.23.1",
    "validator": "10.10.0",
    "webpack": "4.26.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-css-modules": "^1.1.0",
    "jest-localstorage-mock": "^2.4.0"
  }
}
